# Test environment configuration
# This file provides minimal configuration for running tests without real API keys

# Disable search providers that require API keys
SEARCH_PROVIDERS=""

# Enable free APIs for testing
ENABLE_FREE_APIS=true

# Disable features that require external services
ENABLE_REDIS=false
ENABLE_DATABASE=false
ENABLE_PROMETHEUS=false
ENABLE_SNAPSHOT=false

# Set minimal resource limits for tests
MAX_COST_USD=0.01
MIN_EVIDENCE_CARDS=2
MAX_BACKFILL_ATTEMPTS=1

# Use test output directory
OUTPUT_DIR=test_outputs
CHECKPOINT_DIR=test_outputs/checkpoints

# Quick timeouts for tests
HTTP_TIMEOUT_SECONDS=5
WALL_TIMEOUT_SEC=60
PROVIDER_TIMEOUT_SEC=5